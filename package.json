    "marked": "^12.0.2",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tslib": "^2.6.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-node": "^1.6.0",
    "vitest": "^1.6.0",
    "concurrently": "^8.2.2"
